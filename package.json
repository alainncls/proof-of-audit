{
  "name": "proof-of-audit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --check '**/*.css' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.md'",
    "format:fix": "prettier --write '**/*.css' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.md'",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
