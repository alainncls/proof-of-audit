{
  "name": "proof-of-audit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --check '**/*.css' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.md'",
    "format:fix": "prettier --write '**/*.css' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.md'",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reown/appkit": "^1.8.17",
    "@reown/appkit-adapter-wagmi": "^1.8.17",
    "@tanstack/react-query": "^5.90.20",
    "@verax-attestation-registry/verax-sdk": "5.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "viem": "^2.45.1",
    "wagmi": "^3.1.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-radar": "^0.10.1"
  },
  "engines": {
    "node": "22.21.1"
  }
}
